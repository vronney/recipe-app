<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="#fdf8f2"/>

  <!-- Bottom tier -->
  <ellipse cx="400" cy="450" rx="180" ry="30" fill="#f5e6d3"/>
  <rect x="220" y="350" width="360" height="100" fill="#f9f0e3"/>
  <ellipse cx="400" cy="350" rx="180" ry="30" fill="#fff8ed"/>

  <!-- Middle tier -->
  <ellipse cx="400" cy="330" rx="130" ry="22" fill="#f5e6d3"/>
  <rect x="270" y="250" width="260" height="80" fill="#f9f0e3"/>
  <ellipse cx="400" cy="250" rx="130" ry="22" fill="#fff8ed"/>

  <!-- Top tier -->
  <ellipse cx="400" cy="230" rx="80" ry="15" fill="#f5e6d3"/>
  <rect x="320" y="170" width="160" height="60" fill="#f9f0e3"/>
  <ellipse cx="400" cy="170" rx="80" ry="15" fill="#fff8ed"/>

  <!-- Autumn decorations - fall flowers -->
  <!-- Orange flowers -->
  <circle cx="350" cy="350" r="12" fill="#ff8c42"/>
  <circle cx="450" cy="350" r="12" fill="#ff8c42"/>
  <circle cx="380" cy="250" r="10" fill="#ff8c42"/>
  <circle cx="420" cy="250" r="10" fill="#ff8c42"/>

  <!-- Red/burgundy flowers -->
  <circle cx="330" cy="360" r="10" fill="#c65d21"/>
  <circle cx="470" cy="360" r="10" fill="#c65d21"/>
  <circle cx="360" cy="260" r="8" fill="#c65d21"/>
  <circle cx="440" cy="260" r="8" fill="#c65d21"/>

  <!-- Yellow/gold flowers -->
  <circle cx="370" cy="370" r="8" fill="#f4a460"/>
  <circle cx="430" cy="370" r="8" fill="#f4a460"/>
  <circle cx="400" cy="240" r="7" fill="#f4a460"/>

  <!-- Green leaves -->
  <ellipse cx="340" cy="355" rx="8" ry="4" fill="#8b7355" transform="rotate(-30 340 355)"/>
  <ellipse cx="460" cy="355" rx="8" ry="4" fill="#8b7355" transform="rotate(30 460 355)"/>
  <ellipse cx="375" cy="255" rx="6" ry="3" fill="#8b7355" transform="rotate(-20 375 255)"/>
  <ellipse cx="425" cy="255" rx="6" ry="3" fill="#8b7355" transform="rotate(20 425 255)"/>

  <!-- Text overlay -->
  <text x="400" y="550" font-family="Georgia, serif" font-size="24" fill="#c65d21" text-anchor="middle" font-weight="bold">3-Tier Fall Wedding Cake</text>
  <text x="400" y="580" font-family="Arial, sans-serif" font-size="16" fill="#5a4633" text-anchor="middle">Example autumn-themed design</text>
</svg>
